<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_nav_graph"
    app:startDestination="@id/lessonsFragment">

    <fragment
        android:id="@+id/lessonsFragment"
        android:name="com.example.fitnesskittest.view.lessons.LessonsFragment"
        android:label="fragment_lessons"
        tools:layout="@layout/fragment_lessons">
        <action
            android:id="@+id/action_lessonsFragment_to_visitsFragment"
            app:destination="@id/visitsFragment" />
    </fragment>
    <fragment
        android:id="@+id/visitsFragment"
        android:name="com.example.fitnesskittest.view.visits.VisitsFragment"
        android:label="fragment_visits"
        tools:layout="@layout/fragment_visits" />
</navigation>